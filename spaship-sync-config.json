{
  "autosync": {
    "enabled": true,
    "targets": [
      {
        "name": "chrome-head",
        "interval": "5s",
        "source": {
          "url": "https://access.redhat.com/services/chrome/head",
          "sub_paths": [
            "/en",
            "/kr",
            "/ja",
            "/zh_CN"
          ]
        },
        "dest": {
          "path": "/home/arkaprovo/spaship-test/chrome/head",
          "filename": "head.html"
        }
      },
      {
        "name": "chrome-header",
        "interval": "5s",
        "source": {
          "url": "https://access.redhat.com/services/chrome/header",
          "sub_paths": [
            "/en",
            "/kr",
            "/ja",
            "/zh_CN"
          ]
        },
        "dest": {
          "path": "/home/arkaprovo/spaship-test/chrome/header",
          "filename": "header.html"
        }
      },
      {
        "name": "chrome-footer",
        "interval": "5s",
        "source": {
          "url": "https://access.redhat.com/services/chrome/footer",
          "sub_paths": [
            "/en",
